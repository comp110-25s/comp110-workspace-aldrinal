def celcius_to_farenheit(degree: int) -> float:
    return (degree * 9 / 5) + 32


def perimeter(length: float, width: float) -> float:
    return length * 2 + width * 2


"""CL04 Notes"""


def fuel_needed(distance: int, mpg: int) -> float:
    distance / mpg


def total_fuel_cost(distane: int, mpg: int, price_per_gallon: int) -> float:
    return fuel_needed(distance=distance, mpg=mpg) * price_per_gallon
